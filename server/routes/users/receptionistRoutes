const express = require('express');

const {
  getReceptionists,
  editReceptionist,
  deleteReceptionist,
  getReceptionist,
} = require('../../controllers/users/receptionistControllers');
const { createReceptionist } = require('../../controllers/authControllers');

const router = express.Router();

router.route('/').get(getReceptionists).post(createReceptionist);
router
  .route('/:id')
  .get(getReceptionist)
  .patch(editReceptionist)
  .delete(deleteReceptionist);
module.exports = router;
